{
	"name" : "Learn helper",
	"version":"1.3",
	"manifest_version" : 2,
	"description":"添加网络学堂Deadline查询功能，修复网络学堂在Chrome下的排版问题",
	"content_scripts":[
		{
			"matches": [
				"*://learn.tsinghua.edu.cn/MultiLanguage/lesson/student/*",
				"*://learn.tsinghua.edu.cn/MultiLanguage/public/bbs/*"
			],
			"js":["learn.js"],
			"all_frames" : true
		},
		{
			"matches":[
				"*://learn.tsinghua.edu.cn/MultiLanguage/lesson/student/mainstudent.jsp"
			],
			"js" : ["main.js"]
		},
		{
			"matches":[
				"*://learn.tsinghua.edu.cn/MultiLanguage/lesson/student/course_locate.jsp*"
			],
			"js" : ["addException.js"]
		},
		{
			"matches":[
				"*://learn.tsinghua.edu.cn/MultiLanguage/public/bbs/*"

			],
			"js" : ["editor.js"],
			"all_frames" : true
		}
	],
	"permissions" : ["*://learn.tsinghua.edu.cn/*", "tabs"],
	"options_page": "options.html",
	"icons": {
		"16" : "L48.png",
		"48" : "L48.png",
		"128" : "L128.png"
	},
	"web_accessible_resources":[
		"learn.inject.js",
		"libs/editor/*"
	]
}
